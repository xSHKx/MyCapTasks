list = []

n = int(input("Please enter the Total Number of List Elements: "))
for i in range(1, n + 1):
    value = int(input("Please enter the Value of %d Element : " %i))
    list.append(value)

print("\nPositive Numbers in this List are : ")
for a in range(n):
    if(list[a] >= 0):
        print(list[a], end = '   ')
